{
  "dataset_dir" : "Tomato-Merged",
  "checkpoint_filepath": "MobileVITCLAHE",
  "add_dense": true,
  "img_height": 224,
  "img_width": 224,
  "batch_size": 16,
  "epochs": 100,
  "n_classes": 10,
  "seed": 42,
  "fig_format": ".png",
  "data_augmentations":
  {
    "TRAIN_AUG": true,
    "VALID_AUG": false,
    "TEST_AUG": false,
    "isVIT": true,
    "applyCLAHE": true,
    "rotation_range": 20,
    "horizontal_flip": true,
    "width_shift_range": 0.2,
    "height_shift_range": 0.2,
    "shear_range": 0.2
  },
  "model_configuration":
  {
    "backbone_name": "mobilevitsmall",
    "optimizer": "adam"
  },
  "learning_rates":
  {
    "initial_lr" : 1e-5,
    "max_lr" : 0.0001
  },
  "version": "1.0.0"
}
